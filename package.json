{
  "name": "whoomp-monorepo",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "latest",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "web": "pnpm --filter web",
    "mobile": "pnpm --filter mobile",
    "sdk": "pnpm --filter @whoomp/sdk",
    "transport-rn": "pnpm --filter @whoomp/transport-rn",
    "transport-web": "pnpm --filter @whoomp/transport-web"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1"
  }
}